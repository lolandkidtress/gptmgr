<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.openapi.Dao.ApiKeyMapper">
  <resultMap id="BaseResultMap" type="com.openapi.Model.ApiKey">
    <result column="id" jdbcType="VARCHAR" property="id" />
    <result column="deleteflg" jdbcType="BIGINT" property="deleteflg" />
    <result column="createtime" jdbcType="TIMESTAMP" property="createtime" />
    <result column="updatetime" jdbcType="TIMESTAMP" property="updatetime" />

    <result column="apikey" jdbcType="VARCHAR" property="apikey" />
    <result column="email" jdbcType="VARCHAR" property="email" />

  </resultMap>

  <sql id="Base_Column_List">
    id, deleteflg, createtime, updatetime,apikey, email
  </sql>


  <select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List"/>
    from ApiKey
    where id = #{id,jdbcType=VARCHAR}
  </select>


  <insert id="insert" parameterType="com.openapi.Model.ApiKey">
    insert into ApiKey
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="createtime != null">
        createtime,
      </if>
      <if test="apikey != null">
        apikey,
      </if>
      <if test="email != null">
        email,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=VARCHAR},
      </if>
      <if test="createtime != null">
        #{createtime,jdbcType=TIMESTAMP},
      </if>
      <if test="apikey != null">
        #{apikey,jdbcType=VARCHAR}
      </if>
      <if test="email != null">
        #{email,jdbcType=VARCHAR}
      </if>
    </trim>
  </insert>

  <sql id="fromSearchSql">
    from ApiKey
    where 1=1
    and deleteflg = 0
  </sql>

  <select id="search" parameterType="Map" resultType="com.openapi.Model.ApiKey">
    select
    <include refid="Base_Column_List"/>
    <include refid="fromSearchSql"/>

  </select>

  <select id="searchCount" parameterType="Map" resultType="Integer">
    select count(1)
    <include refid="fromSearchSql"/>
  </select>

  <select id="searchOne" parameterType="Map" resultType="com.openapi.Model.ApiKey">
    select
    <include refid="Base_Column_List"/>
    <include refid="fromSearchSql"></include>
    limit 1
  </select>


</mapper>
